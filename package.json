{
	"name": "browsernode",
	"version": "0.4.4",
	"description": "A powerful browser automation library with AI agent capabilities",
	"keywords": [
		"browser",
		"automation",
		"ai",
		"langchain",
		"playwright",
		"agent"
	],
	"license": "MIT",
	"homepage": "https://github.com/leoning60/browsernode",
	"repository": {
		"type": "git",
		"url": "https://github.com/leoning60/browsernode.git"
	},
	"bugs": {
		"url": "https://github.com/leoning60/browsernode/issues"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"bin": {
		"browsernode-cli": "./bin/cli.js"
	},
	"workspaces": ["examples"],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		},
		"./llm": {
			"types": "./dist/llm/index.d.ts",
			"import": "./dist/llm/index.js"
		},
		"./browser": {
			"types": "./dist/browser/index.d.ts",
			"import": "./dist/browser/index.js"
		},
		"./browser/types": {
			"types": "./dist/browser/types.d.ts",
			"import": "./dist/browser/types.js"
		},
		"./agent/views": {
			"types": "./dist/agent/views.d.ts",
			"import": "./dist/agent/views.js"
		}
	},
	"files": ["dist", "bin", "README.md", "LICENSE"],
	"scripts": {
		"build": "tsc -p tsconfig.build.json && cp src/agent/system_prompt.md dist/agent/ && cp src/dom/dom_tree/index.js dist/dom/dom_tree/ && chmod +x bin/cli.js",
		"prepublishOnly": "npm run build",
		"test": "vitest",
		"cli": "tsx src/tokens/cli.ts"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@types/blessed": "^0.1.25",
		"@types/node": "^22.13.13",
		"@types/yargs": "^17.0.33",
		"typescript": "^5.8.2"
	},
	"peerDependencies": {
		"typescript": "^5.8.2"
	},
	"dependencies": {
		"@anthropic-ai/sdk": "^0.56.0",
		"@google/genai": "^1.8.0",
		"@gradio/client": "^1.15.6",
		"@langchain/anthropic": "^0.3.21",
		"@langchain/core": "^0.3.64",
		"@langchain/google-genai": "^0.2.10",
		"@langchain/ollama": "^0.2.1",
		"@mistralai/mistralai": "^1.7.4",
		"@types/cors": "^2.8.19",
		"@types/express": "^5.0.3",
		"@types/turndown": "^5.0.5",
		"blessed": "^0.1.81",
		"blessed-contrib": "^4.11.0",
		"canvas": "^2.11.2",
		"class-transformer": "^0.5.1",
		"clipboardy": "^4.0.0",
		"commander": "^12.1.0",
		"cors": "^2.8.5",
		"dotenv": "^16.6.1",
		"groq": "^3.96.0",
		"langchain": "^0.3.20",
		"lefthook": "^1.11.9",
		"lodash": "^4.17.21",
		"ollama": "^0.5.16",
		"openai": "^5.8.2",
		"patchright": "^1.52.5",
		"playwright": "^1.52.0",
		"posthog-node": "^5.5.0",
		"reflect-metadata": "^0.2.2",
		"sharp": "^0.34.3",
		"systeminformation": "^5.27.7",
		"terminal-image": "^3.1.1",
		"tiktoken": "^1.0.21",
		"ts-node": "^10.9.2",
		"tsx": "^4.19.3",
		"turndown": "^7.2.0",
		"uuid": "^11.1.0",
		"vitest": "^3.0.9",
		"winston": "^3.17.0",
		"ws": "^8.18.3",
		"yargs": "^18.0.0",
		"zod": "^3.24.2",
		"zod-to-json-schema": "^3.24.5"
	}
}
